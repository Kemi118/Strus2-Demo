<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	
	<constant name="struts.custom.i18n.resources" value="i18n"></constant>

	<package name="testvalidation" extends="struts-default">
		<action name="testvalidation" class="com.luodexin.struts2.testvalidation.TestValidationAction">
			<result>/success.jsp</result>
			<result name="input">/testvalidation.jsp</result>
		</action>
	</package>
	
	<package name="testIDCard" extends="struts-default">
		<action name="testIdcard" class="com.luodexin.struts2.testIDCard.IDCardAction">
			<result >/success.jsp</result>
			<result name="input">/testIDCard.jsp</result>
		</action>
			
	</package>

</struts>
